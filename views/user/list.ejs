<table class="table table-striped">
    <thead>
        <tr>
            <th>User Name</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Role</th>
        </tr>
    </thead>
    <tbody>
        <%
        for(var _index=0; _index < users.length; _index++) {
            var _user = users[_index];
        %>
        
        <tr>
            <td><%=_user.userName%></td>
            <td><%=_user.firstName%></td>
            <td><%=_user.lastName%></td>
            <td><%=_user.role%></td>
        </tr>
        <%}%>
    </tbody>
</table>